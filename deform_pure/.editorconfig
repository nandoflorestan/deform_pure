# http://editorconfig.org

root = true

[*]
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf
charset = utf-8

[*.py]
indent_style = space
indent_size = 4

[*.rst]
indent_style = space
indent_size = 4

[*.pt]
indent_style = space
indent_size = 2
